CCOPTS = -O2 -Wall -fomit-frame-pointer -std=gnu99 -march=armv7
OPTS = -DGSPORT_LITTLE_ENDIAN -DHAVE_TFE -DTOGGLE_STATUS -DHAVE_ATBRIDGE
SUFFIX =
NAME = gsplusx
LDFLAGS =
LDOPTS =
LD = g++
EXTRA_LIBS = -lXext -ldl
EXTRA_SPECIALS =

AS = cc
PERL = perl

XOPTS = -I/usr/X11R6/include
